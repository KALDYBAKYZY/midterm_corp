# PostgreSQL DSN for database/sql
DATABASE_URL=postgres://postgres:password@localhost:5432/taskdb?sslmode=disable

# GORM DSN (can be same as DATABASE_URL)
GORM_DATABASE_URL=host=localhost user=postgres password=password dbname=taskdb port=5432 sslmode=disable

# Which repository to use: "sql" or "gorm"
REPO_DRIVER=gorm

# Server port
PORT=8080
